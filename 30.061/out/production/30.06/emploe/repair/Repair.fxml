            <TextField fx:id="tfid" />
            <TextField fx:id="tfdate" layoutX="10.0" layoutY="10.0" />
            <TextField fx:id="tfcustumer" layoutX="10.0" layoutY="35.0" />
            <TextField fx:id="tfmaster" layoutX="10.0" layoutY="10.0" />
