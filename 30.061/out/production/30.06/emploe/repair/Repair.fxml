         <children>
            <Label text="id" />
            <Label layoutX="10.0" layoutY="10.0" text="date" />
            <Label layoutX="10.0" layoutY="27.0" text="custumer" />
            <Label layoutX="10.0" layoutY="10.0" text="master" />
            <Label layoutX="10.0" layoutY="27.0" text="device" />
            <Label layoutX="10.0" layoutY="44.0" text="material" />
            <Label layoutX="10.0" layoutY="95.0" text="dop_info" />
         </children></VBox>
      <VBox layoutX="83.0" layoutY="13.0" prefHeight="143.0" prefWidth="226.0" spacing="6.0">
